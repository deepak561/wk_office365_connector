<odoo>
<data noupdate="0">
	<record model="ir.module.category" id="module_office365_odoo_connector">
		<field name="name">Wk Office365 Connector</field>
		<field name="description">Office365 Odoo Connector</field>
		<field name="sequence">20</field>
	</record>

	<record id="group_office365_user" model="res.groups">
		<field name="name">User</field>
		<field name="comment">User will have read access to the Wk Office365 Connector.</field>
		<field name="category_id" ref="module_office365_odoo_connector"/>
	</record>

	<record id="group_office365_manager" model="res.groups">
		<field name="name">Manager</field>
		<field name="comment">Full Access on Wk Office365 Connector</field>
		<field name="category_id" ref="module_office365_odoo_connector"/>
		<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
		 <field name="implied_ids" eval="[(4, ref('group_office365_user'))]"/>
	</record>
</data>
</odoo>
